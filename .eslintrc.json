{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "getBasePath": "readonly",
    "getCurrentPage": "readonly",
    "addItemToCart": "readonly",
    "updateCartCounter": "readonly",
    "saveCartToStorage": "readonly",
    "getCartFromStorage": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-empty": "warn"
  }
}

